<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ddit.member.mypage.mapper.MemberMypageMapper">
	
	<update id="updTel" parameterType="hashMap">
		UPDATE V_MBER_USERS
		SET MBER_MBTLNUM = #{mberMbtlnum}
		WHERE 1=1
		AND USER_ID = #{userId}
	</update>
	
	<update id="updPW" parameterType="hashMap">
		UPDATE V_MBER_USERS
		SET USER_PASSWORD = #{userPassword}
		WHERE 1=1
		AND USER_ID = #{userId}
	</update>
	
	<update id="updNcnm" parameterType="hashMap">
		UPDATE V_MBER_USERS
		SET USER_NCNM = #{userNcnm}
		WHERE 1=1
		AND USER_ID = #{userId}
	</update>
	
	<update id="updPhoto" parameterType="hashMap">
		UPDATE V_MBER_USERS
		SET MBER_PROFL_PHOTO = #{mberProflPhoto}
		WHERE 1=1
		AND USER_ID = #{userId}
	</update>

</mapper>