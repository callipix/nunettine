package kr.or.ddit.todaymeeting;

import java.util.List;

import lombok.Getter;
import lombok.Setter;
public class Scroll<T> {

	private List<T> msgList;
	
	private int currentPage;
	private int roomNo;
	private int totalSize;
	
	private int pageSize;
	
	private boolean hasMore;
	
	public Scroll(int roomNo , int currentPage , int size , List<T> content){
		this.roomNo = roomNo;
		this.currentPage = currentPage;
		this.msgList = content;
		
	}
	
	public Scroll() {
	}

	public int getTotalSize() {
		return totalSize;
	}

	public void setTotalSize(int totalSize) {
		
		this.totalSize = totalSize;
	}


	public List<T> getMsgList() {
		return msgList;
	}

	public void setMsgList(List<T> msgList) {
		this.msgList = msgList;
	}

	public int getCurrentPage() {
		return currentPage;
	}

	public void setCurrentPage(int currentPage) {
		this.currentPage = currentPage;
	}

	public int getPageSize() {
		return pageSize;
	}

	public void setPageSize(int pageSize) {
		this.pageSize = pageSize;
	}

	public boolean isHasMore() {
		return hasMore;
	}

	public void setHasMore(boolean hasMore) {
		this.hasMore = hasMore;
	}
	
	
		
}
