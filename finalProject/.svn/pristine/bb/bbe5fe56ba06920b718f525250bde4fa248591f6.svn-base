    $('.nav-item').click(function() {
    	$('.nav-item').find('.nav-link').removeClass('active');
        $(this).find('.nav-link').addClass('active');

        var index = $(this).index();
        $('.profile-feed').hide();
        $('.profile-feed').eq(index).show();
    });

		
$(function(){
