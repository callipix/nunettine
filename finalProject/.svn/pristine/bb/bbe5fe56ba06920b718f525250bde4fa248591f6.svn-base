            confirmButtonColor: '#4B49AC', // confrim 버튼 색깔 지정
            cancelButtonColor: '#d33', // cancel 버튼 색깔 지정
            confirmButtonText: '확인', // confirm 버튼 텍스트 지정
        }).then((result) => {
            if (result.isConfirmed) {
                history.back(); // 확인 버튼을 눌렀을 때만 이전 페이지로 돌아가기
            }
