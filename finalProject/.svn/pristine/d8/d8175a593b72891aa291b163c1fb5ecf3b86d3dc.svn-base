$(function(){
	$("#memberLogout").on("click",function(){
		let memLogout = confirm("로그아웃하시겠습니까?");
		if(memLogout){
			location.href="/member/memberLogout";
		}
	})
	
	$("#proLogout").on("click",function(){
		let proLogout = confirm("로그아웃하시겠습니까?");
		if(proLogout){
			location.href="/pro/proLogout";
		}
	})
	
